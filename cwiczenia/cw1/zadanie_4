// Deivid Kovalevskij. Student

arms(){
  arms[]
  j = 0
  i = 100
  while(i < 1000){
    x = i % 10
    y = (i / 10) % 10
    z = (i / 100) % 10
    
    if(i==((x*x*x)+(y*y*y)+(z*z*z))){
      arms[j] = i
      j++
      }
    i++
    }
    
  while(i < 10000){
    v = i % 10
    x = (i / 10) % 10
    y = (i / 100) % 10
    z = (i / 1000) % 10
    
    if(i==((v*v*v*v)+(x*x*x*x)+(y*y*y*y)+(z*z*z*z))){
      arms[j] = i
      j++
      }
    i++
    }
    
    return arms[]
}
